{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/static/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/static/js/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/static/images/$1"
    },
    {
      "src": "/fonts/(.*)",
      "dest": "/static/fonts/$1"
    },
    {
      "src": "/Upload/(.*)",
      "dest": "/static/images/Upload/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "myeyelevel_django.settings"
  }
}
